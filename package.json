        "@carbon/colors": "^10.4.0",
        "@carbon/elements": "^10.6.0",
        "@carbon/grid": "^10.6.0",
        "@carbon/icons-vue": "^10.6.0",
        "@carbon/layout": "^10.5.0",
        "@carbon/motion": "^10.4.0",
        "@carbon/type": "^10.5.0",
        "@carbon/vue": "^2.16.0",
        "axios": "^0.19.0",
        "carbon-components": "^10.7.2",
        "core-js": "^2.6.5",
        "cross-env": "^5.1",
        "d3": "^5.12.0",
        "dotenv": "^8.2.0",
        "font-awesome": "^4.7.0",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.13",
        "open-sans-all": "^0.1.3",
        "path": "^0.12.7",
        "node-sass": "^4.13.1",
        "resolve-url-loader": "^2.3.1",
        "sass-loader": "^7.1.0",
        "vue": "^2.6.10",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.1",
        "webpack": "^4.41.2"
    },
    "devDependencies": {
        "vue-template-compiler": "^2.6.10",
